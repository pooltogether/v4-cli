{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "max-params": "off",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "operator-linebreak": "off",
    "semi": [
      "error",
      "always"
    ],
    "unicorn/filename-case": [
      "error",
      {
        "case": "camelCase"
      }
    ]
  }
}
